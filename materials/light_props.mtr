models/mapobjects/light_props/candelabra_blue
{
    qer_editorimage    models/mapobjects/light_props/candelabra_blue.png
	noShadows
	alphashadow
    
	diffusemap         models/mapobjects/light_props/candelabra_blue.png
	normalMap		models/mapobjects/feudal/NULL_nm.png
	specularmap	    models/mapobjects/light_props/candelabra_blue.png
}

models/mapobjects/light_props/candelabra_blue_light
{
	qer_editorImage		models/mapobjects/cpd/objective_fx.png
	nonsolid
	noshadows
	translucent
	twosided
	
	{
		map		models/mapobjects/cpd/objective_fx.png
		tcmod scroll 0 -0.2
		blend add
	}  
}

