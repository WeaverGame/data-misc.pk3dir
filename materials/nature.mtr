{
    qer_editorimage    models/mapobjects/nature/rock1.png
    
	diffusemap         models/mapobjects/nature/rock1.png
	specularmap	    models/mapobjects/nature/rock1_s.png
	bumpmap            models/mapobjects/nature/rock1_nm.png
}

textures/trak/nature/rock
{
    qer_editorimage    textures/trak/nature/rock.png
		xmap_vertexscale 2
    
	diffusemap         textures/trak/nature/rock.png
	specularmap	    textures/trak/nature/rock_s.png
	bumpmap            textures/trak/nature/rock_nm.png
}

textures/trak/nature/dirt1
{
	qer_editorimage    textures/trak/nature/dirt1.png
	xmap_surfaceModel models/mapobjects/terrain_stones/small_rock_1a.ase 64 0.02 0.3 1.5 0 25 1    
	xmap_surfaceModel models/mapobjects/nature/thistle.ase 64 0.05 0.3 1.5 0 25 1    
	xmap_surfaceModel models/mapobjects/nature/grass4.ase 32 0.3 0.3 1.5 0 25 1    

	diffusemap         textures/trak/nature/dirt1.png
	specularmap	    textures/trak/nature/dirt1_s.png
	bumpmap            textures/trak/nature/dirt1_nm.png
}

