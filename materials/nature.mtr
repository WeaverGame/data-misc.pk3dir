models/mapobjects/nature/rock1
{
    qer_editorimage    models/mapobjects/nature/rock1.png
    
	diffusemap         models/mapobjects/nature/rock1.png
	specularmap	    models/mapobjects/nature/rock1_s.png
	bumpmap            models/mapobjects/nature/rock1_nm.png
}

models/mapobjects/nature/fernleaf
{
	qer_editorImage		models/mapobjects/nature/fernleaf.tga
	xmap_backShader		models/mapobjects/nature/fernleaf_back
	xmap_lightmapBrightness 2
	noShadows
	noselfshadow
	translucent
	nonsolid

	{
		stage		diffuseMap
		map		models/mapobjects/nature/fernleaf.tga
		alphatest 0.5
	}
}

