textures/trak/nature/leaf1
{
	qer_editorImage		textures/trak/nature/leaf1.png
	xmap_backshader		textures/trak/nature/leaf1
	noselfshadow
	translucent
	noimpact
	nonsolid
	deformVertexes wave 30 sin 0 8 0 .2 

	{
		stage		diffusemap
		map		textures/trak/nature/leaf1.tga
		depthwrite
		rgbGen identity
		alphatest 0.5
	}
}

