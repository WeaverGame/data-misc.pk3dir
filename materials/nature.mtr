textures/trak/nature/grass3
{
	qer_editorImage		textures/trak/nature/grass3.tga
	noShadows
	nonsolid
	deformvertexes autosprite2
	xmap_vertexscale 2


	{
		stage		diffuseMap
		map		textures/trak/nature/grass3.tga
		alphatest 0.5
	}
}

