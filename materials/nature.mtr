models/mapobjects/nature/glowplant1
{
	qer_editorImage		models/mapobjects/nature/glowplant1.png
	xmap_backshader 	models/mapobjects/nature/glowplant1_inv
	xmap_surfaceLight	6000
	xmap_lightSubdivide	16
	xmap_lightImage		models/mapobjects/nature/glowplant1_blend.png
	xmap_lightRGB 0.75 0.95 1
	noShadows
	nonsolid
	twosided

	{
		stage		diffuseMap
		map		models/mapobjects/nature/glowplant1.png
		depthwrite
		rgbGen identityLighting
		alphatest 0.5
	}
	{
		map		models/mapobjects/nature/glowplant1_blend.png
		blend		add
	}
}

