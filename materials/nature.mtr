textures/trak/nature/grass3
{
	qer_editorImage		textures/trak/nature/grass3.tga
	noShadows
	noselfshadow
	nonsolid
	deformvertexes autosprite2
	xmap_vertexscale 2


	{
		stage		diffuseMap
		map			textures/trak/nature/grass3.tga
		alphatest 0.5
	}
}

models/mapobjects/nature/grass4
{
	qer_editorImage		models/mapobjects/nature/grass4.tga
	xmap_backshader		models/mapobjects/nature/grass4
	noshadows
	noselfshadow
	alphashadow
	xmap_lightmapSampleOffset 8
	xmap_forceMeta
	q3map_lightmapBrightness 1.5

	{
		stage		diffuseMap
		map		models/mapobjects/nature/grass4.tga
		alphatest 0.5
	}
}


